{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": false, // 由Rollup插件处理声明生成
    "declarationMap": false,
    "sourceMap": false
  },
  "include": ["lib/**/*"],
  "exclude": ["node_modules", "dist"]
}
